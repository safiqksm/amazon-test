	public void usVelocidade(Leopardo leopardo) {
		leopardo.correRapido();
	}
	
	public void usVelocidade(Cachorro cachorro) {
		cachorro.correRapido();
	}
	
	public void usVelocidade(Gato gato) {
		gato.correRapido();
	}
}
